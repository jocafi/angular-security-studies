{
  "name": "security-studies",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.json --ssl --ssl-key key.pem --ssl-cert cert.pem",
    "start-server": "DEBUG=uan* && ./node_modules/.bin/ts-node ./server/server.ts --secure",
    "server": "DEBUG=uan* && ./node_modules/.bin/nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "hash": "node ./demos/hash.js",
    "hash-salt": "node ./demos/hash-salt.js",
    "verify-password": "node ./demos/verify-password.js",
    "jwt-hs256": "node ./demos/jwt-hs256.js",
    "jwt-check-hs256": "node ./demos/jwt-check-hs256.js",
    "jwt-rs256": "node ./demos/jwt-rs256.js",
    "jwt-check-rs256": "node ./demos/jwt-check-rs256.js",
    "csrf-server": "./node_modules/.bin/http-server -c-1 .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint-fix": "tslint --fix -c tslint.json -p tsconfig.json",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.125",
    "argon2": "^0.21.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "password-validator": "^4.1.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/debug": "^4.1.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.0.0",
    "codelyzer": "~4.5.0",
    "command-line-args": "^4.0.6",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.19.0",
    "protractor": "~5.4.0",
    "ts-node": "^8.1.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
